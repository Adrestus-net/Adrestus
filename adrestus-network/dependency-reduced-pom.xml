<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>Adrestus-build</artifactId>
    <groupId>io.Adrestus.v1</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>adrestus-network</artifactId>
  <name>adrestus-network</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-surefire-plugin</artifactId>
          <version>2.22.2</version>
          <configuration>
            <forkCount>1</forkCount>
            <threadCount>1</threadCount>
            <reuseForks>false</reuseForks>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.awaitility</groupId>
      <artifactId>awaitility</artifactId>
      <version>4.2.0</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.32</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <netty.version>4.1.106.Final</netty.version>
    <activej.version>5.4.3</activej.version>
    <ok.http.version>4.9.3</ok.http.version>
    <gson.version>2.9.0</gson.version>
    <util.version>1.0-SNAPSHOT</util.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <ledger.version>1.0-SNAPSHOT</ledger.version>
    <trie.version>1.0-SNAPSHOT</trie.version>
    <java.version>11</java.version>
    <maven.compiler.source>${java.version}</maven.compiler.source>
    <crypto.version>1.0-SNAPSHOT</crypto.version>
    <config.version>1.0-SNAPSHOT</config.version>
    <maven.compiler.target>${java.version}</maven.compiler.target>
    <jeromq.version>0.5.3</jeromq.version>
  </properties>
</project>
